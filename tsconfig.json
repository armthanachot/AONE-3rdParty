{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "strictNullChecks":false,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
